@article{hades_RICH,
title = "Ring recognition in the {HADES} second-level trigger",
journal = "Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment",
volume = "433",
number = "1",
pages = "268 - 273",
year = "1999",
issn = "0168-9002",
doi = "https://doi.org/10.1016/S0168-9002(99)00333-2",
url = "http://www.sciencedirect.com/science/article/pii/S0168900299003332",
author = "J. Lehnert and R. Becker and I. Fröhlich and B. Krieg and W. Kühn and C. Lichtblau and E. Lins and M. Petri and M.-A. Pleier and J. Ritman and C. Salz and M. Traxler",
keywords = "{HADES}, RICH detector, Second-level trigger, electron detection",
abstract = "The online ring recognition processor of the {HADES} second level trigger is described. It performs a search for Cherenkov rings of fixed diameter using the hit pattern information from the RICH detector. A complete pad plane is processed in less than 10μs with a ring finding efficiency greater than 90\%. In combination with the information from the other detector subsystems, an event reduction factor of about 100 is obtained by the second level trigger."
}

